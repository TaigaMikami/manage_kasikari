<% item      = kasikari.item %>
<% from_user = kasikari.from_user %>
<% to_user   = kasikari.to_user %>

<div class="time_show">
start-date(<%= kasikari.start_date%>)ã€œend-date(<%= kasikari.end_date%>)
</div>
<li>
  <div class="kasikari_from_user">
    <%= link_to from_user do %>
      <%= thumbnail_for from_user, size: 100 %>
      <%= from_user.name%>
    <% end %>
  </div>

  <div class="kasikari_item">
    <%= link_to item do %>
      <%= thumbnail_for item, size: 100 %>
